{
    "name": "paypal-e2e",
    "version": "1.0.0",
    "description": "E2E Test suite for the PayPal plugin",
    "main": "index.js",
    "scripts": {
        "e2e:run": "npx playwright test --workers=1",
        "e2e:run:single": "npx playwright test --workers=1 -g",
        "e2e:run:headed": "npx playwright test --headed --workers=1",
        "e2e:run:debug": "DEBUG=pw:api npx playwright test --debug",
        "e2e:run:debug:test": "npx playwright test --debug -g",
        "e2e:codegen": "npx playwright codegen"
    },
    "dependencies": {
        "@playwright/test": "^1.35.1",
        "dotenv": "^16.3.1",
        "mysql": "^2.18.1"
    },
    "devDependencies": {
        "eslint-config-standard": "^17.1.0"
    }
}
